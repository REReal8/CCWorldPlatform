local T_ModuleRegistry = {}
local corelog = require "corelog"

local ModuleRegistry = require "module_registry"

function T_ModuleRegistry.T_All()
    T_ModuleRegistry.T_registerAndGetObject()
end

function T_ModuleRegistry.T_registerAndGetObject()
    -- prepare test
    corelog.WriteToLog("* ModuleRegistry.registerObject() and ModuleRegistry.getObject() tests")
    local object1 = {name = "object1"}
    local object2 = {name = "object2"}
    local registry = ModuleRegistry:getInstance()

    -- test valid

    -- Register the objects with the registry
    registry:registerObject("object1", object1)
    registry:registerObject("object2", object2)

    -- Get the objects back from the registry
    local retrievedObject1 = registry:getObject("object1")
    local retrievedObject2 = registry:getObject("object2")

    -- Use assertions to confirm that the retrieved objects match the original objects
    assert(retrievedObject1 == object1, "Retrieved object 1 does not match original object 1")
    assert(retrievedObject2 == object2, "Retrieved object 2 does not match original object 2")

    -- cleanup test
end

return T_ModuleRegistry

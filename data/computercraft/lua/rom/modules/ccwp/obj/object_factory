local ObjectFactory = {}

local URL = require "obj_url"
local Block = require "obj_block"
local LayerRectangle = require "obj_layer_rectangle"
local ItemInventory = require "obj_item_inventory"
local Chest = require "mobj_chest"

local classes = {
    URL = URL,
    Block = Block,
    LayerRectangle = LayerRectangle,
    ItemInventory = ItemInventory,
    Chest = Chest,
}

function ObjectFactory.create(objType, objTable)
    local class = classes[objType]
    if class then
        return class:new(objTable)
    else
        return nil
    end
end

return ObjectFactory
